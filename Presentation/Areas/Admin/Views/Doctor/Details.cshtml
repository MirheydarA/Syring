@using Business.ViewModels.Admin.Doctor
@model DoctorDetailsVM

<h3>Name</h3>
<span>@Model.Name</span>

<h3>Qualification</h3>
<span>@Html.Raw(Model.Qualification)</span>

<h3>Phone Number</h3>
<span>@Model.PhoneNumber</span>

<h3>Email</h3>
<span>@Model.Email</span>

<h3>Description</h3>
<span>@Html.Raw(Model.Description)</span>

<h3>WorkingTime</h3>
<span>@Html.Raw(Model.WorkingTime)</span>

<h3>Photo</h3>
<span>
    <img class="w-25" src="~/assets/images/@Model.Photo" />
</span>


<h3>Roles</h3>
@foreach (var dutyName in Model.Duty.Name)
{
    <span>@dutyName</span>
}

